<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd">





    <bean name="StamShip" class="spring.course.exercise_1.model.Ship">
        <property name="engine">
            <bean class="spring.course.exercise_1.model.StamEngine"/>
        </property>
    </bean>

    <bean name="Enterprise" class="spring.course.exercise_1.model.Ship">
        <property name="engine">
            <bean class="spring.course.exercise_1.model.WarpEngine"/>
        </property>
    </bean>



</beans>